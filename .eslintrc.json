{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "import/no-cycle": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "no-underscore-dangle": "off",
      "import/no-extraneous-dependencies": "off"
    },
    "ignorePatterns": [
      "dist/",
      "build/",
      "config.js"
    ]
  }